//
// Do any local configuration here
//

// Consider adding the 1918 zones here, if they are not used in your
// organization
//include "/etc/bind/zones.rfc1918";

zone "<<ZONENAME>>" {
        type master;
        file "/etc/bind/zones/<<ZONEFILENAME>>";
        };

zone "<<REVZONENAME>>" {
        type master;
        file "/etc/bind/zones/<<REVZONEFILENAME>>";
        };

